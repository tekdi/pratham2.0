{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "noEmitOnError": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    
  },
  "include": [
     "**/*.js", "**/*.jsx",  "next.config.mjs",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types/global.d.ts",
    "cypress/e2e/app.cy.js",
    "module.config.js"
  ],
  "exclude": ["node_modules", "temp_eliminated_features"]
}
